<%- include('../_partial/_header'); -%>


<div class="row">
    <div class="col-md-6">
        <h1>My Watches</h1>
    </div>
</div>

<div class="row">
    <div class="col-md-6" style="margin-bottom:10px;">
        <a href="/watches/add" type="button" class="btn btn-default">New Watch</a>
    </div>
</div>

<!--List all the posts-->
<% watches.forEach(function(watch) { %>
    <div class="col-sm-4 col-md-4">
        <div class="thumbnail">
            <img src="data:image/svg+xml;base64,"
                alt="logo"
                style="height:200px;width:100%;display:block"
                >
            <div class="caption">
                <h3><%= watch.brand %></h3>
                <p><%= watch.model %></p>
                <p><a href="#" class="btn btn-primary" role="button">Read</a> 
                    <a href="#" class="btn btn-default" role="button">Edit</a>
                </p>
            </div>
        </div>
    </div>

<% }) %>




<%- include ('../_partial/_footer'); -%>